{
  "name": "git-tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon -r dotenv/config ./app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "npm run migrate:u && npm run migrate:s",
    "migrate:dus": "npm run migrate:d && npm run migrate:u && npm run migrate:s",
    "migrate:u": "sequelize db:migrate --debug",
    "migrate:s": "sequelize db:seed:all --debug",
    "migrate:d": "sequelize db:migrate:undo:all --debug",
    "migrate:reset-db": "sequelize db:drop --debug && sequelize db:create --debug"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.20"
  }
}
